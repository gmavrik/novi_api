		{
			"name": "Get Account Session",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "8e169393-bc6f-4afb-88dc-c12ad7df5a1a",
						"exec": [
							"// pm.response.json() and JSON.parse() does not work with big int\r",
							"// So the solution below helped by adding quotes to big numbers\r",
							"var responseBigIntsChangedToStrings = pm.response.text().replace(/([^\"\\w-])([-0-9.]{8,100})([^\"\\w-])/g, '$1\\\"$2\\\"$3');\r",
							"var jsonResponse = [...responseBigIntsChangedToStrings.matchAll(/{.*}/g)];\r",
							"var jsonData = JSON.parse(jsonResponse);\r",
							"\r",
							"pm.environment.set(\"Account_SessionId\", jsonData.sessionId);\r",
							""
						],
						"type": "text/javascript"
					}
				},
