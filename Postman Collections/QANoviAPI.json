		{
			"name": "Process Transaction Stake",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "26a85c6a-9cb0-4f04-a734-73f86ce33d21",
						"exec": [
							"console.log(\"---------------- Push - Stake ----------------\")\r",
							"var body = JSON.parse(interpolate(pm.request.body.toString()));\r",
							"\r",
							"const moment = require('moment');\r",
							"const uuid = require('uuid');\r",
							"\r",
							"const transactionId = uuid.v4();\r",
							"const timestamp = moment.utc().toJSON();\r",
							"const roundId = uuid.v4().replace(/-/g,\"\");\r",
							"\r",
							"console.log(\"Transaction\",transactionId)\r",
							"console.log(\"Timestamp\",timestamp)\r",
							"console.log(\"RoundId\",roundId)\r",
							"\r",
							"pm.environment.set(\"Push_TransactionId\", transactionId);\r",
							"pm.environment.set(\"Push_RoundId\", roundId);\r",
							"pm.environment.set(\"Push_AmountToCancel\", body.actions[0].amount);\r",
							"\r",
							"function interpolate (value) {\r",
							"    const {Property} = require('postman-collection');\r",
							"    return Property.replaceSubstitutions(value, pm.variables.toObject());\r",
							"}\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
